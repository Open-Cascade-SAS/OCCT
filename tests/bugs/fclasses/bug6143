#######################################################################################
# OSD::SetSignal : not all FLT exceptions are catched
# Correct handling of exceptions on WNT
#######################################################################################

pload QAcommands

set BugNumber OCC6143

dsetsignal 1
set IsDone [catch {set aResult [OCC6143]} result]
dsetsignal

if { ${IsDone} != 0 } {
    puts "result = ${result}"
    puts "Faulty ${BugNumber}"
} else {
    if { [string first "TestExcept: Successfull completion" ${aResult} ] != -1 } {
	puts "OK ${BugNumber}"
    } else {
	puts "Faulty ${BugNumber}"
    }
}

